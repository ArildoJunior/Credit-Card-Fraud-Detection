CREATE TABLE creditcard(  
    id INT NOT NULL AUTO_INCREMENT COMMENT 'Primary Key',
    ctime FLOAT COMMENT 'Time of transaction',
    v1 FLOAT COMMENT 'Var with PCA',
    v2 FLOAT COMMENT 'Var with PCA',
    v3 FLOAT COMMENT 'Var with PCA',
    v4 FLOAT COMMENT 'Var with PCA',
    v5 FLOAT COMMENT 'Var with PCA',
    v6 FLOAT COMMENT 'Var with PCA',
    v7 FLOAT COMMENT 'Var with PCA',
    v8 FLOAT COMMENT 'Var with PCA',
    v9 FLOAT COMMENT 'Var with PCA',
    v10 FLOAT COMMENT 'Var with PCA',
    v11 FLOAT COMMENT 'Var with PCA',
    v12 FLOAT COMMENT 'Var with PCA',
    v13 FLOAT COMMENT 'Var with PCA',
    v14 FLOAT COMMENT 'Var with PCA',
    v15 FLOAT COMMENT 'Var with PCA',
    v16 FLOAT COMMENT 'Var with PCA',
    v17 FLOAT COMMENT 'Var with PCA',
    v18 FLOAT COMMENT 'Var with PCA',
    v19 FLOAT COMMENT 'Var with PCA',
    v20 FLOAT COMMENT 'Var with PCA',
    v21 FLOAT COMMENT 'Var with PCA',
    v22 FLOAT COMMENT 'Var with PCA',
    v23 FLOAT COMMENT 'Var with PCA',
    v24 FLOAT COMMENT 'Var with PCA',
    v25 FLOAT COMMENT 'Var with PCA',
    v26 FLOAT COMMENT 'Var with PCA',
    v27 FLOAT COMMENT 'Var with PCA',
    v28 FLOAT COMMENT 'Var with PCA',
    amount FLOAT COMMENT 'Value of transaction',
    class FLOAT COMMENT 'Var with classes 1= fraud, 0= not fraud',
    PRIMARY KEY(id)
);